{
    "compileOnSave": false,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "jsx": "react",
        "sourceMap": true
    },
    "filesGlob": [
        "./**/*.ts",
        "../typings/**/*.d.ts",
        "!./node_modules/**/*.ts"
    ],
    "files": [
        "src/app/actions.ts",
        "src/app/App.ts",
        "src/app/components/_helpers.ts",
        "src/app/constants.ts",
        "src/app/dispatcher.ts",
        "src/app/services/api.ts",
        "src/app/services/dataService.ts",
        "src/app/stores/chirps.ts",
        "src/app/stores/store.ts",
        "typings/angularjs/angular.d.ts",
        "typings/chai/chai.d.ts",
        "typings/flux/flux.d.ts",
        "typings/jquery/jquery.d.ts",
        "typings/lodash/lodash.d.ts",
        "typings/microsoft-ajax/microsoft.ajax.d.ts",
        "typings/mocha/mocha.d.ts",
        "typings/node/node.d.ts",
        "typings/object-assign/object-assign.d.ts",
        "typings/radium/radium.d.ts",
        "typings/react/react-global.d.ts",
        "typings/react/react.d.ts",
        "typings/sharepoint/SharePoint.d.ts",
        "typings/sinon/sinon.d.ts",
        "typings/tsd.d.ts"
    ],
    "exclude": []
}
